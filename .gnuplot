set loadpath '~/.config/gnuplot/gnuplot-palettes'
load 'spectral.pal'

set border lw 3 lc rgb "#81A1C1"
set key textcolor rgb "#81A1C1"

set linetype 1 lc rgb "#BF616A" lw 5 pt 1
set linetype 2 lc rgb "#D08770" lw 5 pt 1
set linetype 3 lc rgb "#EBCB8B" lw 5 pt 1
set linetype 4 lc rgb "#A3BE8C" lw 5 pt 1
set linetype 5 lc rgb "#B48EAD" lw 5 pt 1
set linetype cycle 5


